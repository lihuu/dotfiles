[alias]
	st = status
	br = branch
	ci = commit
	cm = commit
	ck = checkout
[user]
	email = 1449488533qq@gmail.com
	name = SilentStorm
[credential]
	helper = manager-core
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	autocrlf = input
	longpaths = true
    editor = vim
[http "https://github.com"]
	proxy = http://127.0.0.1:7891
[https "https://github.com"]
    proxy = http://127.0.0.1:7891
[i18n]
	logoutputencoding = utf-8
[i18n "commit"]
	encoding = utf-8
[gui]
	encoding = utf-8
[diff]
    submodule = log
[http]
	postBuffer = 3194304000
	lowSpeedLimit = 0
	lowSpeedTime = 999999

